{
  "name": "vuetom-ui",
  "version": "0.2.2",
  "description": "A Vue3 UI component library",
  "main": "dist/vuetom-ui.ts",
  "module": "dist/vuetom-ui.esm.js",
  "scripts": {
    "b": "export NODE_ENV=development && npm run build:clean && npm run build:umd",
    "build": "export NODE_ENV=production && npm run build:clean && npm run build:umd && npm run build:esm",
    "build:umd": "npx rollup -c rollup.config.js",
    "build:esm": "npx rollup -c rollup.config.esm.js",
    "build:clean": "rimraf ./dist",
    "build:theme": "gulp --require sucrase/register/ts -f src/components/gulpfile.ts",
    "pub": "npm publish",
    "docs": "npm run -C docs dev"
  },
  "keywords": [
    "vuetom-ui",
    "vuetom",
    "ui"
  ],
  "author": "lauset",
  "license": "MIT",
  "files": [
    "dist",
    "src",
    "types"
  ],
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.17.10",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.2.1",
    "@vue/babel-preset-jsx": "^1.2.4",
    "@vue/compiler-sfc": "^3.2.33",
    "algoliasearch": "^4.4.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "chokidar": "^1.7.0",
    "clipboard-copy": "^4.0.1",
    "cross-env": "^7.0.2",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cssmin": "^0.2.0",
    "gulp-sass": "^5.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.70.2",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-esbuild": "^3.0.2",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup-plugin-vue": "6.0.0",
    "sass": "^1.49.0",
    "sucrase": "^3.20.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "date-fns": "^2.28.0"
  }
}
